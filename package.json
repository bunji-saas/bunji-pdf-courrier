{
  "name": "pdf-courriers",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "yarn run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write . | grep -v \" (unchanged)\" | cat",
    "lint": "eslint \"{src,test}/**/*.ts\" && prettier --check .",
    "lint:apply": "eslint \"{src,test}/**/*.ts\" --fix && prettier --check .",
    "release": "release-it"
  },
  "engines": {
    "node": "^18.17.1 || ^20.17.0"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.4.2",
    "release-it": "^18.1.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.19.1"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.4.4",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-pdf-html": "^2.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
